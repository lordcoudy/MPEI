\par      static struct tm   dtPrevious ;
\par      BOOL               bChange ;
\par      HDC                hdc ;
\par      PAINTSTRUCT        ps ;
\par      time_t             lTime ;
\par      struct tm         *datetime ;
\par 
\par      switch (iMsg)
\par           \{
\par           case WM_CREATE :
\par                time (&lTime) ;
\par                datetime = localtime (&lTime) ;
\par 
\par                dtPrevious = * datetime ;
\par                return 0 ;
\par 
\par           case WM_SIZE :
\par                cxClient = LOWORD (lParam) ;
\par                cyClient = HIWORD (lParam) ;
\par                return 0 ;
\par 
\par           case WM_TIMER :
\par                time (&lTime) ;
\par                datetime = localtime (&lTime) ;
\par 
\par                bChange = datetime->tm_hour != dtPrevious.tm_hour ||
\par                          datetime->tm_min  != dtPrevious.tm_min ;
\par 
\par                hdc = GetDC (hwnd) ;
\par 
\par                SetIsotropic (hdc, cxClient, cyClient) ;
\par 
\par                SelectObject (hdc, GetStockObject (WHITE_PEN)) ;
\par                DrawHands (hdc, &dtPrevious, bChange) ;
\par }\pard \s18\qj\sa40\sl247\slmult1\widctlpar {\f4 
\par                SelectObject (hdc, GetStockObject (BLACK_PEN)) ;
\par                DrawHands (hdc, datetime, TRUE) ;
\par 
\par                ReleaseDC (hwnd, hdc) ;
\par 
\par                dtPrevious = *datetime ;
\par                return 0 ;
\par 
\par           case WM_PAINT :
\par                hdc = BeginPaint (hwnd, &ps) ;
\par 
\par                SetIsotropic (hdc, cxClient, cyClient) ;
\par                DrawClock    (hdc) ;
\par                DrawHands    (hdc, &dtPrevious, TRUE) ;
\par 
\par                EndPaint (hwnd, &ps) ;
\par                return 0 ;
\par 
\par           case WM_DESTROY :
\par                KillTimer (hwnd, ID_TIMER) ;
\par                PostQuitMessage (0) ;
\par                return 0 ;
\par           \}
\par      return DefWindowProc (hwnd, iMsg, wParam, lParam) ;
\par      \}
