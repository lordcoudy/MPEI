\par      RECT        rect ;
\par      struct tm  *datetime ;
\par      time_t      lTime ;
\par 
\par      time (&lTime) ;
\par      datetime = localtime (&lTime) ;
\par      
\par      iLength = wsprintf (cBuffer, "  %s  %d%s%02d%s%02d  \\r\\n",
\par                (PSTR) szWday + 4 * WDAY,
\par                iDate == 1 ? MDAY  : iDate == 2 ? YEAR  : MONTH, (PSTR) sDate,
\par                iDate == 1 ? MONTH : iDate == 2 ? MONTH : MDAY,  (PSTR) sDate,
\par                iDate == 1 ? YEAR  : iDate == 2 ? MDAY  : YEAR) ;
\par 
\par      if (iTime == 1)
\par           iLength += wsprintf (cBuffer + iLength, "  %02d%s%02d%s%02d  ",
\par                                HOUR, (PSTR) sTime, MIN, (PSTR) sTime, SEC) ;
\par      else
\par           iLength += wsprintf (cBuffer + iLength, "  %d%s%02d%s%02d %s  ",
\par                                (HOUR % 12) ? (HOUR % 12) : 12,
\par                                (PSTR) sTime, MIN, (PSTR) sTime, SEC,
\par                                (PSTR) sAMPM [HOUR / 12]) ;
\par 
\par      GetClientRect (hwnd, &rect) ;
\par      DrawText (hdc, cBuffer, -1, &rect, DT_CENTER | DT_NOCLIP) ;
\par      \}
