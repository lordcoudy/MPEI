\par void SetIsotropic (HDC hdc, int cxClient, int cyClient)
\par      \{
\par      SetMapMode (hdc, MM_ISOTROPIC) ;
\par      SetWindowExtEx (hdc, 1000, 1000, NULL) ;
\par      SetViewportExtEx (hdc, cxClient / 2, -cyClient / 2, NULL) ;
\par      SetViewportOrgEx (hdc, cxClient / 2,  cyClient / 2, NULL) ;
\par      \}
\par 
\par void RotatePoint (POINT pt[], int iNum, int iAngle)
\par      \{
\par      int   i ;
\par      POINT ptTemp ;
\par 
\par      for (i = 0 ; i < iNum ; i++)
\par           \{
\par           ptTemp.x = (int) (pt[i].x * cos (TWOPI * iAngle / 360) +
\par                             pt[i].y * sin (TWOPI * iAngle / 360)) ;
\par 
\par           ptTemp.y = (int) (pt[i].y * cos (TWOPI * iAngle / 360) -
\par                             pt[i].x * sin (TWOPI * iAngle / 360)) ;
\par 
\par           pt[i] = ptTemp ;
\par           \}
\par      \}
\par 
\par void DrawClock (HDC hdc)
\par      \{
\par      int   iAngle ;
\par      POINT pt[3] ;
\par 
\par      for (iAngle = 0 ; iAngle < 360 ; iAngle += 6)
\par           \{
\par           pt[0].x =   0 ;
\par           pt[0].y = 900 ;
\par 
\par           RotatePoint (pt, 1, iAngle) ;
\par 
\par           pt[2].x = pt[2].y = iAngle % 5 ? 33 : 100 ;
\par 
\par           pt[0].x -= pt[2].x / 2 ;
\par           pt[0].y -= pt[2].y / 2 ;
\par 
\par           pt[1].x  = pt[0].x + pt[2].x ;
\par           pt[1].y  = pt[0].y + pt[2].y ;
\par 
\par           SelectObject (hdc, GetStockObject (BLACK_BRUSH)) ;
\par 
\par           Ellipse (hdc, pt[0].x, pt[0].y, pt[1].x, pt[1].y) ;
\par           \}
\par      \}
\par 
\par void DrawHands (HDC hdc, struct tm *datetime, BOOL bChange)
\par      \{
\par      static POINT pt[3][5] = \{ 0, -150, 100, 0, 0, 600, -100, 0, 0, -150,
\par                                0, -200,  50, 0, 0, 800,  -50, 0, 0, -200,
\par                                0,    0,   0, 0, 0,   0,    0, 0, 0,  800 \} ;
\par      int          i, iAngle[3] ;
\par      POINT        ptTemp[3][5] ;
\par 
\par      iAngle[0] = (datetime->tm_hour * 30) % 360 + datetime->tm_min / 2 ;
\par      iAngle[1] =  datetime->tm_min  *  6 ;
\par }\pard \s18\qj\sa40\sl245\slmult1\widctlpar {\f4      iAngle[2] =  datetime->tm_sec  *  6 ;
\par 
\par      memcpy (ptTemp, pt, sizeof (pt)) ;
\par 
\par      for (i = bChange ? 0 : 2 ; i < 3 ; i++)
\par           \{
\par           RotatePoint (ptTemp[i], 5, iAngle[i]) ;
\par 
\par           Polyline (hdc, ptTemp[i], 5) ;
\par           \}
\par      \}
