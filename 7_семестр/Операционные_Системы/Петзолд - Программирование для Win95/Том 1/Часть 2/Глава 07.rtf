\par void SizeTheWindow (int *pxStart,  int *pyStart,
\par                     int *pxClient, int *pyClient)
\par      \{
\par      HDC        hdc ;
\par      TEXTMETRIC tm ;
\par 
\par      hdc = CreateIC ("DISPLAY", NULL, NULL, NULL) ;
\par      GetTextMetrics (hdc, &tm) ;
\par      DeleteDC (hdc) ;
\par 
\par      *pxClient = 2 * GetSystemMetrics (SM_CXDLGFRAME) + 16*tm.tmAveCharWidth ;
\par      *pxStart  =     GetSystemMetrics (SM_CXSCREEN)   - *pxClient ;
\par      *pyClient = 2 * GetSystemMetrics (SM_CYDLGFRAME) + 2*tm.tmHeight ;
\par      *pyStart  =     0 ;
\par      \}
