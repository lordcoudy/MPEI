\par                params.hwnd = hwnd ;
\par                params.cyChar = HIWORD (GetDialogBaseUnits ()) ;
\par                _beginthread (Thread1, 0, &params) ;
\par                return 0 ;
\par 
\par           case WM_SIZE :
\par                params.cyClient = HIWORD (lParam) ;
\par                return 0 ;
