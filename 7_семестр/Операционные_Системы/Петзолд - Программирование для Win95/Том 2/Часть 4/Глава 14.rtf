\par // Window 4: Display circles of random radii
\par // -----------------------------------------
\par 
\par void Thread4 (PVOID pvoid)
\par      \{
\par      HDC     hdc ;
\par      int     iDiameter ;
\par      PPARAMS pparams ;
\par 
\par      pparams = (PPARAMS) pvoid ;
\par 
