\par VOID Thread (PVOID pvoid)
\par      \{
\par      HBRUSH hBrush ;
\par      HDC    hdc ;
\par      int    xLeft, xRight, yTop, yBottom, iRed, iGreen, iBlue ;
\par 
\par      while (TRUE)
\par           \{
\par           if (cxClient != 0 || cyClient != 0)
\par                \{
\par                xLeft   = rand () % cxClient ;
\par                xRight  = rand () % cxClient ;
\par                yTop    = rand () % cyClient ;
\par                yBottom = rand () % cyClient ;
\par                iRed    = rand () & 255 ;
\par                iGreen  = rand () & 255 ;
\par                iBlue   = rand () & 255 ;
\par 
\par                hdc = GetDC (hwnd) ;
\par                hBrush = CreateSolidBrush (RGB (iRed, iGreen, iBlue)) ;
\par                SelectObject (hdc, hBrush) ;
\par 
\par                Rectangle (hdc, min (xLeft, xRight), min (yTop, yBottom),
\par                                max (xLeft, xRight), max (yTop, yBottom)) ;
\par 
\par                ReleaseDC (hwnd, hdc) ;
\par                DeleteObject (hBrush) ;
\par                \}
\par           \}
\par      \}
\par }\pard \s18\qj\sa40\sl247\slmult1\widctlpar {\f4 LRESULT CALLBACK WndProc (HWND hwnd, UINT iMsg, WPARAM wParam, LPARAM lParam)
