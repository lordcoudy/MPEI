\par // Window 2: Display increasing sequence of prime numbers
\par // ------------------------------------------------------
\par 
\par void Thread2 (PVOID pvoid)
\par      \{
\par      char    szBuffer[16] ;
\par      int     iNum = 1, iLine = 0, i, iSqrt ;
\par      HDC     hdc ;
\par      PPARAMS pparams ;
\par 
\par      pparams = (PPARAMS) pvoid ;
\par 
\par      while (!pparams->bKill)
\par           \{
\par           do
\par                \{
\par                if (++iNum < 0)
\par                     iNum = 0 ;
\par 
\par                iSqrt = (int) sqrt (iNum) ;
\par 
\par                for (i = 2 ; i <= iSqrt ; i++)
\par                     if (iNum % i == 0)
\par                          break ;
\par                \}
\par           while (i <= iSqrt) ;
\par 
\par           iLine = CheckBottom (pparams->hwnd,   pparams->cyClient,
\par                                pparams->cyChar, iLine) ;
\par 
\par           wsprintf (szBuffer, "%d", iNum) ;
\par 
\par           hdc = GetDC (pparams->hwnd) ;
\par 
\par           TextOut (hdc, 0, iLine * pparams->cyChar,
\par                    szBuffer, strlen (szBuffer)) ;
\par 
\par           ReleaseDC (pparams->hwnd, hdc) ;
\par 
\par           iLine++ ;
\par           \}
\par      _endthread () ;
\par      \}
\par }\pard \s18\qj\sa40\sl247\slmult1\widctlpar {\f4 
