\par }\pard \s18\qj\sa40\sl245\slmult1\widctlpar {\f4      static int   iNum, iLine ;
\par      static short cyClient ;
\par      char         szBuffer[16] ;
\par      HDC          hdc ;
\par 
\par      switch (iMsg)
\par           \{
\par           case WM_SIZE :
\par                cyClient = HIWORD (lParam) ;
\par                return 0 ;
\par 
\par           case WM_TIMER :
\par                if (iNum < 0)
