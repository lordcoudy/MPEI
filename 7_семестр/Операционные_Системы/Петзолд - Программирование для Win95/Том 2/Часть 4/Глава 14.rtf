\par // Window 3: Display increasing sequence of Fibonacci numbers
\par // ----------------------------------------------------------
\par 
\par void Thread3 (PVOID pvoid)
\par      \{
\par      char    szBuffer[16] ;
\par      int     iNum = 0, iNext = 1, iLine = 0, iTemp ;
\par      HDC     hdc ;
\par      PPARAMS pparams ;
\par 
\par      pparams = (PPARAMS) pvoid ;
\par 
\par      while (!pparams->bKill)
\par           \{
\par           if (iNum < 0)
\par                \{
\par                iNum  = 0 ;
\par                iNext = 1 ;
\par                \}
\par 
\par           iLine = CheckBottom (pparams->hwnd,   pparams->cyClient,
\par                                pparams->cyChar, iLine) ;
\par 
\par           wsprintf (szBuffer, "%d", iNum) ;
\par 
\par           hdc = GetDC (pparams->hwnd) ;
\par 
\par           TextOut (hdc, 0, iLine * pparams->cyChar,
\par                    szBuffer, strlen (szBuffer)) ;
\par 
\par           ReleaseDC (pparams->hwnd, hdc) ;
\par 
\par           iTemp  = iNum ;
\par           iNum   = iNext ;
\par           iNext += iTemp ;
\par 
\par           iLine++ ;
\par           \}
\par      _endthread () ;
\par      \}
