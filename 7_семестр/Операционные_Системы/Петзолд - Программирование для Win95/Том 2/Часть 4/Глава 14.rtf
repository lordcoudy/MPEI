\par int CheckBottom (HWND hwnd, int cyClient, int cyChar, int iLine)
\par      \{
\par      if (iLine * cyChar + cyChar > cyClient)
\par           \{
\par           InvalidateRect (hwnd, NULL, TRUE) ;
\par           UpdateWindow (hwnd) ;
\par           iLine = 0 ;
\par           \}
\par      return iLine ;
\par      \}
\par }\pard \s18\qj\sa40\sl245\slmult1\widctlpar {\f4 // Window 1: Display increasing sequence of numbers
\par // ------------------------------------------------
\par 
\par void Thread1 (PVOID pvoid)
\par      \{
\par      int     iNum = 0, iLine = 0 ;
\par      char    szBuffer[16] ;
\par      HDC     hdc ;
\par      PPARAMS pparams ;
\par 
\par      pparams = (PPARAMS) pvoid ;
\par 
\par      while (!pparams->bKill)
\par           \{
\par           if (iNum < 0)
\par                iNum = 0 ;
\par 
\par           iLine = CheckBottom (pparams->hwnd,   pparams->cyClient,
\par                                pparams->cyChar, iLine) ;
\par 
\par           wsprintf (szBuffer, "%d", iNum++) ;
\par 
\par           hdc = GetDC (pparams->hwnd) ;
\par 
\par           TextOut (hdc, 0, iLine * pparams->cyChar,
\par                    szBuffer, strlen (szBuffer)) ;
\par 
\par           ReleaseDC (pparams->hwnd, hdc) ;
\par 
\par           iLine++ ;
\par           \}
\par      _endthread () ;
\par      \}
\par 
