\par                SendMessage (pparams->hwnd, WM_CALC_DONE, 0, lTime) ;
\par                \}
\par           else
\par                SendMessage (pparams->hwnd, WM_CALC_ABORTED, 0, 0) ;
\par           \}
\par      \}
