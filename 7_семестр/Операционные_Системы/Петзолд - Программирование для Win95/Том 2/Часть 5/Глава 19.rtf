\par }\pard \s18\qj\sa40\sl247\slmult1\widctlpar {\f4    STRPROG.C -- Program using STRLIB dynamic link library
\par }\pard \s18\qj\sa40\sl250\slmult1\widctlpar {\f4                 (c) Charles Petzold, 1996
\par   --------------------------------------------------------*/
\par 
\par #include <windows.h>
\par #include <string.h>
\par #include "strprog.h"
\par #include "strlib.h"
\par 
\par #define MAXLEN 32
\par #define WM_DATACHANGE WM_USER
\par 
\par typedef struct
\par      \{
\par      HDC  hdc ;
\par      int  xText ;
\par      int  yText ;
\par      int  xStart ;
\par      int  yStart ;
\par      int  xIncr ;
\par      int  yIncr ;
\par      int  xMax ;
\par      int  yMax ;
\par      \}
\par      CBPARAM ;
\par 
\par LRESULT CALLBACK WndProc (HWND, UINT, WPARAM, LPARAM) ;
\par 
\par char szAppName[] = "StrProg" ;
\par char szString[MAXLEN] ;
\par 
\par int WINAPI WinMain (HINSTANCE hInstance, HINSTANCE hPrevInstance,
\par                     PSTR szCmdLine, int iCmdShow)
\par      \{
\par      HWND        hwnd ;
\par      MSG         msg ;
\par      WNDCLASSEX  wndclass ;
\par 
\par      wndclass.cbSize        = sizeof (wndclass) ;
\par      wndclass.style         = CS_HREDRAW | CS_VREDRAW ;
\par      wndclass.lpfnWndProc   = WndProc ;
\par      wndclass.cbClsExtra    = 0 ;
\par      wndclass.cbWndExtra    = 0 ;
\par      wndclass.hInstance     = hInstance ;
\par      wndclass.hIcon         = LoadIcon (NULL, IDI_APPLICATION) ;
\par      wndclass.hCursor       = LoadCursor (NULL, IDC_ARROW) ;
\par      wndclass.hbrBackground = (HBRUSH) GetStockObject (WHITE_BRUSH) ;
\par      wndclass.lpszMenuName  = szAppName ;
\par      wndclass.lpszClassName = szAppName ;
\par      wndclass.hIconSm       = LoadIcon (NULL, IDI_APPLICATION) ;
\par 
\par      RegisterClassEx (&wndclass) ;
\par 
\par      hwnd = CreateWindow (szAppName, "DLL Demonstration Program",
\par                           WS_OVERLAPPEDWINDOW,
\par                           CW_USEDEFAULT, CW_USEDEFAULT,
\par                           CW_USEDEFAULT, CW_USEDEFAULT,
\par                           NULL, NULL, hInstance, NULL) ;
\par 
\par      ShowWindow (hwnd, iCmdShow) ;
\par      UpdateWindow (hwnd) ;
\par 
\par      while (GetMessage (&msg, NULL, 0, 0))
\par           \{
\par           TranslateMessage (&msg) ;
\par           DispatchMessage (&msg) ;
\par           \}
\par      return msg.wParam ;
\par      \}
\par 
\par BOOL CALLBACK DlgProc (HWND hDlg, UINT iMsg, WPARAM wParam, LPARAM lParam)
\par      \{
\par      switch (iMsg)
\par           \{
\par           case WM_INITDIALOG :
\par                SendDlgItemMessage (hDlg, IDD_STRING, EM_LIMITTEXT,
\par                                    MAXLEN - 1, 0) ;
\par                return TRUE ;
\par 
\par           case WM_COMMAND :
\par                switch (wParam)
\par                     \{
\par                     case IDOK :
\par                          GetDlgItemText (hDlg, IDD_STRING, szString, MAXLEN) ;
\par                          EndDialog (hDlg, TRUE) ;
\par                          return TRUE ;
\par 
\par                     case IDCANCEL :
\par                          EndDialog (hDlg, FALSE) ;
\par                          return TRUE ;
\par                     \}
\par           \}
\par      return FALSE ;
\par      \}
\par 
\par BOOL CALLBACK EnumCallBack (HWND hwnd, LPARAM lParam)
\par      \{
\par      char szClassName[16] ;
\par 
\par      GetClassName (hwnd, szClassName, sizeof (szClassName)) ;
\par 
\par      if (0 == strcmp (szClassName, szAppName))
\par           SendMessage (hwnd, WM_DATACHANGE, 0, 0) ;
\par 
\par      return TRUE ;
\par      \}
\par 
\par BOOL CALLBACK GetStrCallBack (PSTR pString, CBPARAM *pcbp)
\par      \{
\par      TextOut (pcbp->hdc, pcbp->xText, pcbp->yText,
\par               pString, strlen (pString)) ;
\par 
\par      if ((pcbp->yText += pcbp->yIncr) > pcbp->yMax)
\par           \{
\par           pcbp->yText = pcbp->yStart ;
\par           if ((pcbp->xText += pcbp->xIncr) > pcbp->xMax)
\par                return FALSE ;
\par           \}
\par      return TRUE ;
\par      \}
\par 
\par LRESULT CALLBACK WndProc (HWND hwnd, UINT iMsg, WPARAM wParam, LPARAM lParam)
\par      \{
\par      static HINSTANCE  hInst ;
\par      static int        cxChar, cyChar, cxClient, cyClient ;
\par      CBPARAM           cbparam ;
\par      HDC               hdc ;
\par      PAINTSTRUCT       ps ;
\par      TEXTMETRIC        tm ;
\par 
\par      switch (iMsg)
\par           \{
\par           case WM_CREATE :
\par                hInst = ((LPCREATESTRUCT) lParam)->hInstance ;
\par                hdc   = GetDC (hwnd) ;
\par                GetTextMetrics (hdc, &tm) ;
\par                cxChar = (int) tm.tmAveCharWidth ;
\par                cyChar = (int) (tm.tmHeight + tm.tmExternalLeading) ;
\par                ReleaseDC (hwnd, hdc) ;
\par 
\par                return 0 ;
\par 
\par           case WM_COMMAND :
\par                switch (wParam)
\par                     \{
\par                     case IDM_ENTER :
\par                          if (DialogBox (hInst, "EnterDlg", hwnd, &DlgProc))
\par }\pard \s18\qj\sa40\sl245\slmult1\widctlpar {\f4                               \{
\par                               if (AddString (szString))
\par                                    EnumWindows (&EnumCallBack, 0) ;
\par                               else
\par                                    MessageBeep (0) ;
\par                               \}
\par                          break ;
\par 
\par                     case IDM_DELETE :
\par                          if (DialogBox (hInst, "DeleteDlg", hwnd, &DlgProc))
\par                               \{
\par                               if (DeleteString (szString))
\par                                    EnumWindows (&EnumCallBack, 0) ;
\par                               else
\par                                    MessageBeep (0) ;
\par                               \}
\par                          break ;
\par                     \}
\par                return 0 ;
\par 
\par           case WM_SIZE :
\par                cxClient = (int) LOWORD (lParam) ;
\par                cyClient = (int) HIWORD (lParam) ;
\par                return 0 ;
\par 
\par           case WM_DATACHANGE :
\par                InvalidateRect (hwnd, NULL, TRUE) ;
\par                return 0 ;
\par 
\par           case WM_PAINT :
\par                hdc = BeginPaint (hwnd, &ps) ;
\par 
\par                cbparam.hdc   = hdc ;
\par                cbparam.xText = cbparam.xStart = cxChar ;
\par                cbparam.yText = cbparam.yStart = cyChar ;
\par                cbparam.xIncr = cxChar * MAXLEN ;
\par                cbparam.yIncr = cyChar ;
\par                cbparam.xMax  = cbparam.xIncr * (1 + cxClient / cbparam.xIncr) ;
\par                cbparam.yMax  = cyChar * (cyClient / cyChar - 1) ;
\par 
\par                GetStrings ((PSTRCB) GetStrCallBack, (PVOID) &cbparam) ;
\par 
\par                EndPaint (hwnd, &ps) ;
\par                return 0 ;
\par 
\par           case WM_DESTROY :
\par                PostQuitMessage (0) ;
\par                return 0 ;
\par           \}
\par      return DefWindowProc (hwnd, iMsg, wParam, lParam) ;
\par }\pard \s18\qj\sa40\sl247\slmult1\widctlpar {\f4      \}
